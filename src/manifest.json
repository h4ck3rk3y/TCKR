{
    "name": "TCKR",
    "version": "0.0.1",
    "description": "Get live quotes",
    "manifest_version": 2,
    "content_scripts": [
        {
            "js": [
                "content_script.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "background": {
        "scripts": [
            "hot-reload.js"
        ]
    },
    "web_accessible_resources": [
        "content.js",
        "ticker.js",
        "bloomfilter.js"
    ]
}